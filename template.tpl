___SANDBOXED_JS_FOR_SERVER___

const getEventData = require('getEventData');
const JSON = require("JSON");
const log = require('logToConsole');
const sendHttpRequest = require('sendHttpRequest');


const url = data.webhookUrl;
var message = data.text; //https://hooks.slack.com/services/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


  var attachment = {
      "fallback": data.text
  };

  if (data.color && data.color !== 'none') {
    attachment.color = data.color;
  }

  if (data.eventKeys) { 
    var myFields = [];
    data.eventKeys.forEach(function(k) {
      var myKey = getEventData(k.key);
      var myDisplay = k.displayAs ? k.displayAs : k.key;
       myFields.push({ "title": myDisplay, "value": myKey ? myKey : "(empty)"});
    });
    attachment.fields = myFields;
  }

  var payload={ "text": message };

  if (data.emoji) {
    payload.icon_emoji = data.emoji;
  }
  if (data.channel) {
    payload.channel = data.channel;
  }
  if (data.username) {
    payload.username = data.username;
  }

  payload.attachments = [ attachment ];

  const postBody = JSON.stringify(payload);
  // Sends a POST request to slack
  sendHttpRequest(url, (statusCode, headers, body) => {
    if (statusCode == 200) {
      log('sent message to slack');
    } else {
      log('failed to send message to slack. Response: ' + statusCode);
      data.gtmOnFailure();
    }
  },{headers: {'Content-Type': 'application/json'}, method: 'POST', timeout: 5000}, postBody);


data.gtmOnSuccess();


